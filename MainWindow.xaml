        <Border BorderThickness="1" HorizontalAlignment="Center" Height="50" Margin="231,0,9,9" VerticalAlignment="Bottom" Width="50">
            <Image Margin="11" Source="closeicon.png" Stretch="Fill"/>
        </Border>
        <Button Content="Button" HorizontalAlignment="Center" Margin="231,0,9,9" VerticalAlignment="Bottom" Width="50" Height="50" Opacity="0" Click="Button_Click_3"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="175,0,65,9" VerticalAlignment="Bottom" Width="50" Height="50" Opacity="0" Click="Button_Click_4"/>
        <Border BorderThickness="1" HorizontalAlignment="Left" Height="50" Margin="10,60,0,339" VerticalAlignment="Center" Width="270"/>
        <Button Content="Button" Margin="11,60,9,339" VerticalAlignment="Center" Height="50" Opacity="0" Click="Button_Click_5"/>
